{
    "manifest_version": 2,
    "name": "incognitoSwitch",
    "version": "2022.1.14",
    "browser_action": {
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "incognitoSwitch@quimozillafirefoxgecko.org"
        }
    },
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}